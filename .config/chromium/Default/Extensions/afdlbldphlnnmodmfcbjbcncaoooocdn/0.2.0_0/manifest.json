{
   "background": {
      "scripts": [ "jquery-latest.min.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "description": "Provides a notification when David Colgan is live on Twitch.tv!",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "DVLive",
   "permissions": [ "https://api.twitch.tv/", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.0",
   "web_accessible_resources": [ "icon.png" ]
}
